<h1>Home#index</h1>
<% if user_signed_in? %>
	<%= link_to 'logout', '/logout'%>
	<p> Welcome <%= current_user.email %> </p>
	<% end %>
	<%= %>
<%= form_tag '/create_tweet' do %>
